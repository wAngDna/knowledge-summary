{
  "name": "knowledge-summary1",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write src/",
    "check": "tsno run ./scripts/check.ts",
    "lint:style": "stylelint **/*.{vue,css}",
    "lint:stage": "lint-staged",
    "commit": "git-cz",
    "prepare": "husky install",
    "commitlint": "commitlint --edit",
    "deploy": "gh-pages -d dist -r https://github.com/wAngDna/knowledge-summary.git -b gh-pages"
  },
  "homepage": "http://wAngDna.github.io/knowledge-summary",
  "lint-staged": {
    "*": "prettier --write",
    "*.{vue,ts}": "eslint --fix",
    "*.{html,vue,css,sass,scss}": "stylelint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.3.17",
    "axios": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "fast-glob": "^3.3.1",
    "highlight.js": "^11.8.0",
    "Prism": "^2.0.0",
    "sass": "^1.69.5",
    "view-ui-plus": "^1.3.14",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@commitlint/cz-commitlint": "17.6.7",
    "@rushstack/eslint-patch": "1.3.2",
    "@tsconfig/node18": "18.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "20.4.2",
    "@types/vue-markdown": "^2.2.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "4.3.0",
    "cspell": "6.31.2",
    "cypress": "12.17.2",
    "cz-git": "1.7.0",
    "eslint": "8.45.0",
    "eslint-plugin-cypress": "2.13.3",
    "eslint-plugin-vue": "9.15.1",
    "gh-pages": "^6.0.0",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lint-staged": "13.2.3",
    "npm-run-all": "4.1.5",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "prettier": "3.0.0",
    "start-server-and-test": "^2.0.0",
    "stylelint": "15.10.2",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-vue": "1.0.0",
    "tsno": "2.0.0",
    "typescript": "5.1.6",
    "vite": "4.4.5",
    "vitest": "0.33.0",
    "vue-tsc": "1.8.5",
    "zx": "7.2.3"
  }
}
